{
  "expo": {
    "name": "Asedia",
    "slug": "asedia",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "asedia",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#101013",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.asena.space"
    },
    "android": {
      "package": "com.asena.space",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#101013"
      },
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "73b063a3-1c70-4291-9653-2a51150c88e5"
      }
    }
  }
}
